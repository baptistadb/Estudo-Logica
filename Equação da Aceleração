Algoritmo "Teste-Salario"

Var

  produtoum : caractere
  produtodois : caractere
  precoum: real
  precodois: inteiro
  idade: inteiro
  codigo: inteiro
  genero: caractere


Inicio

      produtoum <- "Computador"
      produtodois <- "TV"
      precoum <- 2100.5
      precodois <- 1830
      idade <- 30
      codigo <- 5291
      genero <- "F"

  escreval ( "Produtos:" )
  escreval (" O produto ", produtoum, " custa R$", precoum:8:2)
  escreval (" O produto ", produtodois, " custa R$", precodois:8:2)
  escreval
  escreval ("Codigo =", codigo)
  escreval
  escreval ("Dados da pessoa: genero ", genero, " e idade ", idade)

Fimalgoritmo
